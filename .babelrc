{
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react"
      ],
      "plugins": ["syntax-dynamic-import"]
    },
    "test": {
      "presets": [
        ["@babel/preset-env", { "targets": { "node": "current" }}],
        "@babel/preset-react"
      ],
      "plugins": ["syntax-dynamic-import"]
    }
  }
}
